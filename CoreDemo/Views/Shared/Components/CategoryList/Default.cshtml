@model List<CategoryBlogandBlogCountDto>;
<div class="tech-btm">
    <h4>Kategoriler</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <a class="categoryLink" href="/Blog/Index/?id=@item.CategoryID" name="@item.CategoryID" style="color:#01cd74;">
                    @item.CategoryName
                    <span class="badge badge-primary badge-pill">@item.NumberofBloginCategory</span>
                </a>
            </li>
        }
    </ul>
</div>